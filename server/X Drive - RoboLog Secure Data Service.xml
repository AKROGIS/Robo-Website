      <Command>C:\Python27\ArcGISx6410.6\python.exe</Command>
      <Arguments>"E:\XDrive\UpdateTools\LogProcessor\ServerSecure.py"</Arguments>
      <WorkingDirectory>E:\XDrive\UpdateTools\LogProcessor</WorkingDirectory>
    </Exec>
  </Actions>
</Task>
